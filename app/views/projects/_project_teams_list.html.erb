<h3 align = center> All Teams </h3>
	<%= link_to 'Add Team',new_team_project_path(@project),class: 'btn btn-primary'%>	
<br><br>

<ul class="list-group">
  <% @project.teams.each do |team| %>
	  <li class="list-group-item d-flex justify-content-between align-items-center">
	    <%= team.name%>
	    <%= link_to 'Remove Team',remove_team_project_path(@project,team: team),method: :delete,class: 'badge badge-danger'%>
	  </li>
  <% end %>
</ul>