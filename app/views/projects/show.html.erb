	      <th scope="col">Status</th>
	      <th scope="col"><%= link_to 'Add Issue','',class: 'btn btn-primary' %></th>
	    </tr>
	  </thead>
	  <tbody>
	    <tr>
	      <th scope="row">1</th>
	      <td>Mark</td>
	      <td>Otto</td>
