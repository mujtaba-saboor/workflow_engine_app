
<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4"><%= @project.name%></h1>
    <p class="lead"><%= @project.project_category%></p>
  </div>
</div>
<ul class="nav nav-tabs">
  <li class="nav-item">
  	<%= link_to 'Issues','#',class: 'nav-link active',id: 'my-id-issue-link'%>    
  </li>
  <li class="nav-item">
    <%= link_to project_display_name(@project),project_users_path(@project),class: 'nav-link',remote: true,id: 'my-id-member-link'%>
  </li>
</ul>
<div class = "my-custom-project-data-area">
