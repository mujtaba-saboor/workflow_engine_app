<div class="col-md-9">
  <div class="profile-head">
    <br>
      <ul class="nav nav-tabs" id="myTab" role="tablist">
          <li class="nav-item">
              <a class="nav-link active">About</a>
          </li>
      </ul>
  </div>
</div>
<br>
<div class="col-md-8">
    <div class="row">
        <div class="col-md-6">
            <label><%= t('users.name_label') %></label>
        </div>
        <div class="col-md-6">
            <p><%= @user.name %></p>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <label><%= t('users.email_label') %></label>
        </div>
        <div class="col-md-6">
            <p><%= @user.email %></p>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <label><%= t('users.role_label') %></label>
        </div>
        <div class="col-md-6">
            <p><%= @user.role %></p>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <label><%= t('users.company_label') %></label>
        </div>
        <div class="col-md-6">
            <p><%= @user.company.name %></p>
        </div>
    </div>
    <br>
    <div>
      <%= button_to t('shared.update_resource', resource_name: 'User'), edit_user_registration_path, method: :get, class: 'btn btn-info' %>
    </div>
</div>
